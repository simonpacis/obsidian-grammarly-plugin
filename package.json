		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"overrides": {
		"@codemirror/highlight": "0.19.7",
		"@codemirror/lang-javascript": "0.19.7",
		"@codemirror/state": "0.19.9",
